{
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Dragonfish",
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**",
                "tslib.js"
            ],
            "smartStep": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch with Debugger",
            "program": "${workspaceFolder}/dist/packages/server/main.js",
            "smartStep": true,
        }
    ]
}
